{
    "extends": "airbnb-base",
    "installedESLint": true,
    "plugins": [
      "import",
      "mocha"
    ],
    "parserOptions": {
      "sourceType": "script"
    },
    "env": {
      "browser": false,
      "node": true
    },
    "rules": {
      "no-use-before-define": "off",
      "no-param-reassign": "off"
    }
}
